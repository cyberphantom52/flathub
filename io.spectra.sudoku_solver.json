{
    "app-id" : "io.tachyon.sudoku_solver",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "43",
    "sdk" : "org.gnome.Sdk",
    "sdk-extensions" : [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command" : "sudoku-solver",
    "finish-args" : [
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland"
    ],
    "build-options" : {
        "append-path" : "/usr/lib/sdk/rust-stable/bin"
    },
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name": "blueprint-compiler",
            "buildsystem": "meson",
            "sources": [
              {
                "type": "git",
                "url": "https://gitlab.gnome.org/jwestman/blueprint-compiler",
                "tag": "v0.6.0"
              }
            ]
        },  
        {
            "name" : "sudoku-solver",
            "builddir" : true,
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "archive",
                    "url" : "https://gitlab.com/cyberphantom52/sudoku-solver/uploads/cec1f630efc18e07974e70e9a0b73fa7/sudoku-solver-1.0.0.tar.xz",
                    "sha256" : "76f85d7e9dc3a1e77888c97649c4a8259f88a61a9dcb8bea0d69c4e40aa8b39c"
                }
            ]
        }
    ]
}
